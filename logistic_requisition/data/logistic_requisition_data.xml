<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
	<function
            eval="('default',False,'warehouse_id', [('logistic.requisition', False)], ref('stock.warehouse0'), True, False, False, False, True)"
            id="purchase_default_set"
            model="ir.values"
            name="set"/>
    </data>
    <data noupdate="1">
        
        <!-- LR line-related subtypes for messaging / Chatter -->
        <record id="mt_request_line_assigned" model="mail.message.subtype">
             <field name="name">Assigned</field>
             <field name="res_model">logistic.requisition.line</field>
             <field name="default" eval="False"/>
             <field name="description">Request Line Assigned</field>
         </record>
        <record id="mt_request_line_quoted" model="mail.message.subtype">
             <field name="name">Quoted</field>
             <field name="res_model">logistic.requisition.line</field>
             <field name="default" eval="False"/>
             <field name="description">Request Line Quoted</field>
         </record>
     </data>
</openerp>